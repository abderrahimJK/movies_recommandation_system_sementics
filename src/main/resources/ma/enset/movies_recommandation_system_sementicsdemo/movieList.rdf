# Movies
ex:movie1 rdf:type schema:Movie ;
    schema:name "Beetlejuice" ;
    schema:datePublished "1988" ;
    schema:duration "PT92M" ;
    schema:director "Tim Burton" ;
    schema:actor "Alec Baldwin, Geena Davis, Annie McEnroe, Maurice Page" ;
    schema:description "A couple of recently deceased ghosts contract the services of a \"bio-exorcist\" in order to remove the obnoxious new owners of their house." ;
    schema:image "https://images-na.ssl-images-amazon.com/images/M/MV5BMTUwODE3MDE0MV5BMl5BanBnXkFtZTgwNTk1MjI4MzE@._V1_SX300.jpg" ;
    schema:genre ex:Comedy, ex:Fantasy .

ex:movie2 rdf:type schema:Movie ;
    schema:name "The Cotton Club" ;
    schema:datePublished "1984" ;
    schema:duration "PT127M" ;
    schema:director "Francis Ford Coppola" ;
    schema:actor "Richard Gere, Gregory Hines, Diane Lane, Lonette McKee" ;
    schema:description "The Cotton Club was a famous night club in Harlem. The story follows the people that visited the club, those that ran it, and is peppered with the Jazz music that made it so famous." ;
    schema:image "https://images-na.ssl-images-amazon.com/images/M/MV5BMTU5ODAyNzA4OV5BMl5BanBnXkFtZTcwNzYwNTIzNA@@._V1_SX300.jpg";
    schema:genre ex:Crime, ex:Drama, ex:Music .

